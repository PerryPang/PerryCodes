<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ak.mapper.TaskResultMapper">

    <resultMap type="com.ak.entity.TTaskResult" id="BaseResultMap">

    </resultMap>
    <insert id="insert" parameterType="com.ak.entity.TTask">
        INSERT INTO `monitor`.`t_task_result` (`hijack_num`, `remoteId`, `max_node`, `result`)
        VALUES (#{hijackNum}, #{remoteId}, #{maxNode}, #{result});
    </insert>

</mapper>

